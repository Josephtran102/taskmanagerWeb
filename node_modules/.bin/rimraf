#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/vietnguyen/Web3/Java Mastery/taskmanager/client/node_modules/rimraf/node_modules:/Users/vietnguyen/Web3/Java Mastery/taskmanager/client/node_modules:/Users/vietnguyen/Web3/Java Mastery/taskmanager/node_modules:/Users/vietnguyen/Web3/Java Mastery/node_modules:/Users/vietnguyen/Web3/node_modules:/Users/vietnguyen/node_modules:/Users/node_modules:/node_modules:/Users/vietnguyen/Web3/Java Mastery/taskmanager/client/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/vietnguyen/Web3/Java Mastery/taskmanager/client/node_modules/rimraf/node_modules:/Users/vietnguyen/Web3/Java Mastery/taskmanager/client/node_modules:/Users/vietnguyen/Web3/Java Mastery/taskmanager/node_modules:/Users/vietnguyen/Web3/Java Mastery/node_modules:/Users/vietnguyen/Web3/node_modules:/Users/vietnguyen/node_modules:/Users/node_modules:/node_modules:/Users/vietnguyen/Web3/Java Mastery/taskmanager/client/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../rimraf/bin.js" "$@"
else
  exec node  "$basedir/../rimraf/bin.js" "$@"
fi
